#!/usr/bin/python3

import sys

# Function to factorize a number using trial division
def factorize(n):
    factors = []
    divisor = 2
    while n > 1:
        while n % divisor == 0:
            factors.append(divisor)
            n //= divisor
        divisor += 1
    return factors

# Read numbers from file and factorize
def factorize_numbers(filename):
    with open(filename, 'r') as file:
        numbers = file.readlines()

    for number in numbers:
        number = int(number.strip())
        factors = factorize(number)
        print(f"{number}={'*'.join(map(str, factors))}")

# Entry point
if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: python script.py <file>")
    else:
        filename = sys.argv[1]
        factorize_numbers(filename)
